{
    "name": "react-redux-webpack-starter",
    "version": "0.0.1",
    "engines": {
        "node": ">=10.0.0"
    },
    "description": "React-Redux-Webpack starter with common tasks and scenarios",
    "scripts": {
        "build": "env-cmd ./.env webpack --config ./config/webpack.prod.js --progress && npm run build:server",
        "build:dev": "webpack --config ./config/webpack.dev.js --progress && npm run build:server",
        "build:server": "webpack --config ./config/webpack.server.js --progress",
        "build:server-dev": "webpack --config ./config/webpack.server.js --progress --watch",
        "clean:build": "rimraf build",
        "prebuild": "npm run clean:build",
        "prestart": "npm run clean:build",
        "start": "npm run build:dev && nodemon ./server-dev.js & npm run build:server-dev",
        "format": "prettier-eslint --single-quote --trailing-comma es5 --tab-width 4 --prettierLast --write \"src/**/*.{js,jsx}\" \"gulp/**/*.js\"",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "build-storybook": "build-storybook",
        "test": "export NODE_ENV=test && jest",
        "screenshot": "export NODE_ENV=screenshot && jest",
        "snapshot": "build-storybook && PERCY_TOKEN=$PERCY_TOKEN percy-storybook --widths=320,1280 --debug",
        "screenshot:ci": "kill-port --port 9001 && (npm run storybook & wait-on tcp:localhost:9001) && export NODE_ENV=screenshot && jest",
        "wait-on": "wait-on",
        "kill-port": "kill-port"
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/.jest/register-context.js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|scss)$": "identity-obj-proxy"
        }
    },
    "nodemonConfig": {
        "watch": [
            "build/server/server.js"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/MadeInHaus/react-redux-webpack-starter.git"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "7.3.4",
        "@babel/plugin-proposal-decorators": "7.3.0",
        "@babel/plugin-syntax-dynamic-import": "7.2.0",
        "@babel/plugin-transform-modules-commonjs": "^7.4.4",
        "@babel/preset-env": "7.3.4",
        "@babel/preset-react": "7.0.0",
        "@percy-io/percy-storybook": "^2.1.0",
        "@storybook/addon-actions": "^5.0.11",
        "@storybook/addon-knobs": "^5.0.11",
        "@storybook/addon-links": "^5.0.11",
        "@storybook/addon-storyshots": "^5.0.11",
        "@storybook/addon-storyshots-puppeteer": "^5.0.11",
        "@storybook/addon-viewport": "^5.0.11",
        "@storybook/addons": "^5.0.11",
        "@storybook/react": "^5.0.11",
        "autoprefixer": "9.4.10",
        "babel-eslint": "10.0.1",
        "babel-jest": "^24.7.1",
        "babel-loader": "8.0.5",
        "babel-plugin-module-resolver": "3.2.0",
        "babel-plugin-require-context-hook": "^1.0.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "babel-plugin-transform-react-remove-prop-types": "0.4.24",
        "copy-webpack-plugin": "5.0.1",
        "css-loader": "2.1.1",
        "eslint": "5.15.1",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-react": "7.12.4",
        "extend": "3.0.2",
        "file-loader": "3.0.1",
        "html-webpack-plugin": "3.2.0",
        "http-proxy": "1.17.0",
        "identity-obj-proxy": "^3.0.0",
        "incstr": "1.2.3",
        "jest": "^24.7.1",
        "jest-cli": "^24.7.1",
        "jest-transform-css": "^2.0.0",
        "kill-port": "^1.4.0",
        "node-sass": "4.11.0",
        "nodemon": "1.18.10",
        "postcss-loader": "3.0.0",
        "prettier": "1.16.4",
        "prettier-eslint": "8.8.2",
        "prettier-eslint-cli": "4.7.1",
        "puppeteer": "^1.15.0",
        "raw-loader": "1.0.0",
        "react-hot-loader": "4.8.0",
        "react-test-renderer": "^16.8.6",
        "resolve-url-loader": "3.0.1",
        "rimraf": "2.6.3",
        "sass-loader": "7.1.0",
        "sass-variable-loader": "0.1.2",
        "storybook-router": "0.3.4",
        "style-loader": "0.23.1",
        "uglifyjs-webpack-plugin": "2.1.2",
        "url-loader": "1.1.2",
        "wait-on": "^3.2.0",
        "webpack": "4.29.6",
        "webpack-cli": "3.2.3",
        "webpack-dev-middleware": "3.6.1",
        "webpack-hot-middleware": "2.24.3",
        "webpack-merge": "4.2.1",
        "webpack-node-externals": "1.7.2"
    },
    "dependencies": {
        "axios": "0.18.0",
        "classnames": "2.2.6",
        "compression": "1.7.3",
        "connect-inject": "0.4.0",
        "debug": "4.1.1",
        "dotenv": "6.2.0",
        "env-cmd": "8.0.2",
        "express": "4.16.4",
        "imagemin-webpack-plugin": "2.4.2",
        "keymirror": "0.1.1",
        "lodash": "4.17.11",
        "mini-css-extract-plugin": "0.5.0",
        "normalize.css": "8.0.1",
        "postcss-scss": "2.0.0",
        "prop-types": "15.7.2",
        "react": "16.8.4",
        "react-animation-components": "3.0.0",
        "react-dom": "16.8.4",
        "react-helmet": "5.2.0",
        "react-redux": "6.0.1",
        "react-router": "4.3.1",
        "react-router-dom": "4.3.1",
        "react-transition-group": "2.6.0",
        "redux": "4.0.1",
        "redux-logger": "3.0.6",
        "redux-thunk": "2.3.0",
        "reselect": "4.0.0",
        "serialize-javascript": "1.6.1"
    }
}
