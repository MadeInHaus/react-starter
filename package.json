{
    "name": "react-starter",
    "version": "1.0.0",
    "engines": {
        "node": ">=10.16.3"
    },
    "description": "React-Redux-Webpack starter with common tasks and scenarios",
    "scripts": {
        "build": "env-cmd -f ./.env webpack --config ./config/webpack.prod.js --progress && npm run build:server",
        "build:dev": "webpack --config ./config/webpack.dev.js --progress && npm run build:server",
        "build:server": "webpack --config ./config/webpack.server.js --progress",
        "build:server-dev": "webpack --config ./config/webpack.server.js --progress --watch",
        "clean:build": "rimraf build",
        "prebuild": "npm run clean:build",
        "prestart": "npm run clean:build",
        "start": "npm run build:dev && nodemon ./server-dev.js & npm run build:server-dev",
        "format": "prettier-eslint --single-quote --trailing-comma es5 --tab-width 4 --prettierLast --write \"src/**/*.{js,jsx}\" \"gulp/**/*.js\"",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "nodemonConfig": {
        "watch": [
            "build/server/server.js"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/MadeInHaus/react-redux-webpack-starter.git"
    },
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Safari versions",
        "last 2 Firefox versions",
        "last 2 iOS versions",
        "> 1% in US",
        "not ie 11",
        "not op_mini all",
        "not dead"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "7.7.7",
        "@babel/core": "7.7.7",
        "@babel/plugin-proposal-class-properties": "7.7.4",
        "@babel/plugin-proposal-decorators": "7.7.4",
        "@babel/plugin-syntax-dynamic-import": "7.7.4",
        "@babel/preset-env": "7.7.7",
        "@babel/preset-react": "7.7.4",
        "@storybook/addon-actions": "^5.2.8",
        "@storybook/addon-knobs": "^5.2.8",
        "@storybook/addon-links": "^5.2.8",
        "@storybook/addons": "^5.2.8",
        "@storybook/react": "^5.2.8",
        "autoprefixer": "9.7.3",
        "babel-eslint": "10.0.3",
        "babel-loader": "8.0.6",
        "babel-plugin-module-resolver": "4.0.0",
        "babel-plugin-transform-react-remove-prop-types": "0.4.24",
        "copy-webpack-plugin": "5.1.1",
        "css-loader": "3.4.1",
        "eslint": "6.8.0",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-react": "7.17.0",
        "extend": "3.0.2",
        "file-loader": "5.0.2",
        "html-webpack-plugin": "3.2.0",
        "http-proxy": "1.18.0",
        "incstr": "1.2.3",
        "node-sass": "4.13.0",
        "nodemon": "2.0.2",
        "postcss-loader": "3.0.0",
        "prettier": "1.19.1",
        "prettier-eslint": "9.0.1",
        "prettier-eslint-cli": "5.0.0",
        "raw-loader": "4.0.0",
        "react-hot-loader": "4.12.18",
        "resolve-url-loader": "3.1.1",
        "rimraf": "3.0.0",
        "sass-loader": "8.0.0",
        "sass-variable-loader": "0.1.2",
        "storybook-router": "0.3.4",
        "style-loader": "1.1.2",
        "uglifyjs-webpack-plugin": "2.2.0",
        "url-loader": "3.0.0",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-dev-middleware": "3.7.2",
        "webpack-hot-middleware": "2.25.0",
        "webpack-merge": "4.2.2",
        "webpack-node-externals": "1.7.2"
    },
    "dependencies": {
        "acorn": "7.1.0",
        "axios": "0.19.1",
        "classnames": "2.2.6",
        "compression": "1.7.4",
        "connect-inject": "0.4.0",
        "core-js": "3.6.2",
        "debug": "4.1.1",
        "dotenv": "8.2.0",
        "env-cmd": "10.0.1",
        "express": "4.17.1",
        "imagemin-webpack-plugin": "2.4.2",
        "keymirror": "0.1.1",
        "lodash": "4.17.15",
        "mini-css-extract-plugin": "0.9.0",
        "normalize.css": "8.0.1",
        "npm-check-updates": "4.0.1",
        "postcss-scss": "2.0.0",
        "prop-types": "15.7.2",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-helmet": "5.2.1",
        "react-redux": "7.1.3",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-transition-group": "4.3.0",
