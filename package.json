{
  "name": "react-redux-webpack-starter",
  "version": "0.0.1",
  "engines": {
    "node": ">=6.0.0"
  },
  "description": "React-Redux-Webpack starter with common tasks and scenarios",
  "scripts": {
    "build": "env-cmd ./.env webpack --config ./config/webpack.prod.js --progress && npm run build:server",
    "build:dev": "webpack --config ./config/webpack.dev.js --progress && npm run build:server",
    "build:server": "webpack --config ./config/webpack.server.js --progress",
    "build:server-dev": "webpack --config ./config/webpack.server.js --progress --watch",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "prestart": "npm run clean:build",
    "start": "npm run build:dev && nodemon ./server-dev.js & npm run build:server-dev",
    "format": "prettier-eslint --single-quote --trailing-comma es5 --tab-width 4 --prettierLast --write \"src/**/*.{js,jsx}\" \"gulp/**/*.js\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "nodemonConfig": {
    "watch": [
      "build/server/server.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MadeInHaus/react-redux-webpack-starter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "3.2.12",
    "@storybook/addon-knobs": "3.2.12",
    "@storybook/addon-links": "3.2.12",
    "@storybook/react": "3.2.12",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-node6": "11.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bundle-loader": "^0.5.5",
    "copy-webpack-plugin": "4.1.1",
    "css-loader": "0.28.7",
    "eslint": "4.8.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-react": "7.4.0",
    "extend": "3.0.1",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "http-proxy": "1.16.2",
    "node-sass": "4.5.3",
    "nodemon": "1.12.1",
    "postcss-loader": "2.0.7",
    "prettier": "1.7.4",
    "prettier-eslint": "8.2.1",
    "prettier-eslint-cli": "4.4.0",
    "raw-loader": "0.5.1",
    "react-hot-loader": "3.0.0",
    "resolve-url-loader": "2.1.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "sass-variable-loader": "0.1.2",
    "storybook-router": "0.3.0",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "1.0.0-beta.3",
    "url-loader": "0.6.2",
    "webpack": "3.7.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-plugin-react-css-modules": "3.3.1",
    "classnames": "2.2.5",
    "compression": "1.7.1",
    "connect-inject": "0.4.0",
    "debug": "3.1.0",
    "dotenv": "4.0.0",
    "env-cmd": "^7.0.0",
    "express": "4.16.2",
    "imagemin-webpack-plugin": "1.5.2",
    "immutable": "3.8.2",
    "keymirror": "0.1.1",
    "lodash": "4.17.4",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-scss": "1.0.2",
    "prop-types": "15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-transition-group": "^2.2.1",
    "react-waypoint": "7.3.1",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "serialize-javascript": "1.4.0"
  }
}
